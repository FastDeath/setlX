print("Which factorial should be computed?");
n := read();

beginComp := now();
f := fac(n);
endComp := now();

beginPrint := now();
print(n,"! := ", fac(n));
endPrint := now();

print("Computing the factorial of ", n, " took \t", (endComp  - beginComp  - 0.0)/1000, " seconds.");
print("Printing the factorial of  ", n, " took \t", (endPrint - beginPrint - 0.0)/1000, " seconds.");

